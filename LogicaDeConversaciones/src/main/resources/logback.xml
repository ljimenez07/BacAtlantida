<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<include resource="org/springframework/boot/logging/logback/base.xml"/>

    <logger name="com.ncubo.ui">
        <level value="INFO" />
    </logger>
    <logger name="com.ncubo">
        <level value="INFO" />
    </logger>
     <logger name="com.ncubo.watson">
        <level value="INFO" />
    </logger>
    <logger name="org.springframework">
        <level value="INFO" />
    </logger>
    <root>
        <level value="INFO" />
        <appender-ref ref="CONSOLE" />
    </root>
	
</configuration>
